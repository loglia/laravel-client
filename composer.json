{
    "name": "retrospekt/laravel-client",
    "description": "For sending logs and such.",
    "homepage": "https://retrospekt.app",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^5.6 || ^7.0",
        "ext-json": "*",
        "monolog/monolog": "^1.24",
        "ramsey/uuid": "^3.8"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7"
    },
    "autoload": {
        "psr-4": {
            "Retrospekt\\LaravelClient\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Retrospekt\\LaravelClient\\LaravelClientServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "prefer-stable": true
}
