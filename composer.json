{
    "name": "retrospekt/laravel-client",
    "description": "For sending logs and such.",
    "homepage": "https://retrospekt.app",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^7.1",
        "ext-json": "*",
        "laravel/framework": "^5.1",
        "monolog/monolog": "^1.0",
        "ramsey/uuid": "^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7",
        "mockery/mockery": "^0 || ^1.0",
        "orchestra/testbench": "3.*"
    },
    "autoload": {
        "psr-4": {
            "Loglia\\LaravelClient\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Loglia\\LaravelClient\\LaravelClientServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "prefer-stable": true
}
